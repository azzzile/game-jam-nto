Gerenciador:
	Login:
	Gamee:

<Login>:
    name: 'login'
    MDLabel:
        id: text_change
        text: 'there will be some text'
        halign: 'center'
        theme_text_color: 'Custom'
        text_color : 148/255,0,211/255,1
        font_style : 'H1'
    MDRectangleFlatIconButton:
        icon: 'android'
        text: 'START GAME'
        user_font_size: '100sp'
        pos_hint: {'center_x':0.5,'center_y':0.7}
        on_release:
            root.manager.current= 'tela'


<Gamee>:
	name:'tela'
	canvas :
		Rectangle:
			size: self.size
			pos: self.pos
			source:'images/cats.png'
	BoxLayout:
		orientation:'vertical'
		Button :
			text:'jump'
			background_normal: "transparent.png"
	        background_down: "transparent.png"
	        id: start_button
	        on_release:
	            root.start_game()
	    BoxLayout:
	    	orientation:'horizontal'
	    	Button :
				text:'left'
				background_normal: "transparent.png"
	        	background_down: "transparent.png"
				on_release:
	            	root.move_left()
			Button :
				text:'right'
				background_normal: "transparent.png"
	        	background_down: "transparent.png"
				on_release:
	            	root.move_right()

